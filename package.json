{
  "name": "webcaptureservice",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "bin": "app.js",
  "engines": {
    "node": ">=18.16.0"
  },
  "scripts": {
    "start": "node app.js",
    "build": "pkg . --public"
  },
  "pkg": {
    "targets": [
      "node18-win-x64",
      "node18-linux-x64"
    ],
    "outputPath": "dist",
    "options": {
      "preBundle": true
    }
  },
  "dependencies": {
    "better-queue": "^3.8.12",
    "chrome-launcher": "^1.1.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "pm2": "^5.4.3",
    "process": "^0.11.10",
    "puppeteer": "^23.1.0",
    "puppeteer-cluster": "^0.24.0",
    "puppeteer-core": "^23.1.1",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
